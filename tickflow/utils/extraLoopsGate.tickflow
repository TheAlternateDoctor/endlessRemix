//Gates
playPataponPracticePatternGate:
    set_cat 0
    0xB2 lives
    if_neq 0
        async_call sequencePataponPractice
        rest 0x18
        0x58 0x100, 0x30
        0xB2<1> lives_enabled
        if 1
            async_call checkA2
        endif
        rest 0x78
        call loadBasketGirlsVanilla
        rest 0x24C
        async_call loadBasketGirlsLate
        async_call loadBasketGirlsLate2
        rest 0xC
        call updateScoreGate
        rest 0xC
        0xB2 lives
        if_neq 0
            0xB2 score
            if_lt maxTurns
                fade 1,1,1
            endif
        endif
    endif
return

