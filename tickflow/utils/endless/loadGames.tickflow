loadSong:
random 100
if_gt 90
	set_sfx 0xA, u"GROUP_WSD_ENDLESS_JAM"
else
    if_gt 85
	    set_sfx 0xA, u"GROUP_WSD_ENDLESS_RICK"
    else
	    set_sfx 0xA, u"GROUP_WSD_ENDLESS"
    endif
endif
return

loadTutorial:
    0x35<2> 8
    if 1
        remove_cellanim 8
    endif
    rest 0xC
    random 2
    switch
    case 0
        set_cellanim 8, u"rom:/cellanim/lesson_v0.zlib", -1
        break
    case 1
        set_cellanim 8, u"rom:/cellanim/lesson_yon_circus.zlib", -1
        break
    endswitch
    0x35<2> 9
    if 0
        set_cellanim 9, u"rom:/cellanim/common.zlib", -1
    endif
    0x35<2> 0xB
    if 0
        set_layout 0xB, u"rom:/layout/lesson.zlib", -1
    endif
return

